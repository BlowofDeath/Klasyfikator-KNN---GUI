{
  "name": "electron-gui",
  "version": "1.0.1",
  "description": "Klasyfikator KNN",
  "author": {
    "name": "Tomasz Jabłoński",
    "email": "tjablonski@codeinq.pl",
    "url": "https://codeinq.pl"
  },
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --linux --windows",
    "dist-linux": "electron-builder --linux deb tar.xz"
    
  },
  "license": "ISC",
  "devDependencies": {
    "electron": "^13.6.6",
    "electron-builder": "^22.4.1"
  },
  "dependencies": {
    "bootstrap": "^4.4.1",
    "jquery": "^3.4.1",
    "popper.js": "^1.16.1",
    "python-shell": "^1.0.8"
  },
  "build": {
    "appId": "pl.codeinq.knn",
    "productName": "Klasyfikator KNN",
    "asar": true,
    "asarUnpack": [
      "python",
      "python-scripts"
    ],
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": true,
      "createDesktopShortcut": true,
      "uninstallDisplayName": "Odinstaluj Klasyfikator KNN"
    },
    "win":{
      "target": "nsis",
      "icon": "build/logo.png",
      "publisherName": "Codeinq.pl"
    }
 
  }
  
  
}
